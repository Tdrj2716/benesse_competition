{
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "printWidth": 100,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",

  "importOrder": ["^(react/(.*)$)|^(react$)", "^@?\\w", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
